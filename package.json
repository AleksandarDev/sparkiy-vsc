{
    "name": "sparkiy-vsc",
    "displayName": "sparkiy.vsc",
    "description": "Sparkiy toolchain for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "Sparkiy",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.sparkiyproj"
    ],
    "main": "./out/src/extension",
    "contributes": {
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}